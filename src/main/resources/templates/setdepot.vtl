
<!DOCTYPE html><html lang="en" class="wf-sourcesanspro-n2-inactive wf-sourcesanspro-n3-inactive wf-sourcesanspro-n4-inactive wf-sourcesanspro-n6-inactive wf-sourcesanspro-n7-inactive wf-sourcesanspro-n9-inactive wf-inactive">
<head>

      <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
      
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <title>XPRESSING </title>
      <meta name="description" content="xpressing - Java based pressing web app.">
      
      
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <meta name="theme-color" content="#f7c824">
      

      <link rel="stylesheet" type="text/css" href="css/base.css">
</head>
<body>


#parse($base)

<div class="banner">
  <div class="container">
    <div class="header clear pad-top-10 pad-bottom-20" style="">
      <a class="logo left" href="#"><img src="data:image/png;base64,"></a>
      <div class="right" style="margin-top: 15px"><img style="margin-right: 10px;" src="data:image/png;base64," class="left"><a href="/"><img style="margin-left: 10px;" src="data:image/png;base64," class="right"></a><span class="right"> Logged </span></div>

    </div>
 
  </div>
</div>


<div class="features-block">
  <div class="container text-center pad-top-60 pad-bottom-0 pad-top-60-m pad-bottom-0-m">
   
<form method="post" action="/savedep">
   <div class="row clear pad-top-20 pad-bottom-0">
      <div class="col-6 col-4-m " style="border:solid 0.5px #f4f4f4;padding: 0px 0px;">
        <div class=" col-12-m header" style="padding: 14px 0px">
          CLIENT
        </div>
        <div class="col-12-m userscontainer">
           <div class="row">
         
            

             <div class=" col-12-m custbloc"> 
                 
                 <div class="col-12-m" style="color: #00aa88;text-align: left;">
                   
           
                  
                     <span class="right neworlist" id="neworlist">LISTE</span>
                     <input type="hidden" name="neworlist" id="hidden" value="new">
                     <input type="hidden" name="artCount" id="hiddenCount" value="1">

                    


                 </div>

                 <div class="col-12-m" id="newuser" style="display: block; color: #00aa88;text-align: left;">
                   
           
                  
                    <input class="userinput" type="text" name="custsname" placeholder="Prenoms"><br>
                    <input class="userinput" type="text" name="custname" placeholder="Nom"><br>
                    <input class="userinput" type="text" name="custaddr" placeholder="Addresse"><br>
                    <input class="userinput" type="number" name="custnum" placeholder="Numero Tel"><br>


                 </div>

                 <div class="col-12-m" id="lisuser" style="display: none;">
                       
                    #foreach($cust in $custs)
                     
                     <div class="col-12-m custdets">
                         <span class="left">$cust.prenom $cust.nom  |  <small style="font-size: 12px;">$cust.numTel</small></span>
                         
                        <input checked="" class="right" style="width: 20px; margin-top: 5px;" type="radio" name="selectuser" value="$cust.id">
                     </div>

                   #end

                 </div>

             </div>

           </div>

        </div>
      </div>
      <div class="push-1-m col-6 col-7-m  " style="border:solid 0.5px #f4f4f4;padding: 0">
              
        <div class=" col-12-m header">
          <span>DETAILS DU DEPOT</span><img id="newart" src="data:image/png;base64," class="right" style="margin-right: 10px;cursor: pointer;"/>
        </div>
        <div class="col-12-m empcontainer">
           
           <div class="row">

             <div class="push-1-m col-10-m" id="arts">
                     

                    <input  style="margin-top: 30px;" class="artinput" type="text" name="art0" placeholder="libellé . Ex : Chemise à rayures bleues..."><br> 
                 

               
             </div>

           </div>

        </div>
      </div>
    </div>
       
         <input class="right enrbtn" style="height:40px;background-color: #00aa88;border:none ;color:white; " type="submit" name="" value="ENREGISTRER"></input>
</form>

  </div>
</div>

 

<div class="footer pad-top-20 pad-bottom-20 fs-6" style="margin-top: 80px;">
  <div class="container">
    <div class="clear">
      <div class="col-6-m">
        <p class="no-margin r-bottom-10">
         <span><sup><em>Cheikh G. Wane - Maguette N. Ndiaye - Shadow </em></sup></span>
        </p>
      </div>
      <div class="col-6-m">
        <ul class="footer-links list-inline text-right-m">
          &copyCopyright Mars - 2017
        </ul>
      </div>
    </div>
  </div>
</div>


<script type="text/javascript">
  
  var sbtn = document.getElementById('neworlist');
  var newbloc = document.getElementById('newuser');
  var listbloc = document.getElementById('lisuser');
  var hidden = document.getElementById('hidden');
  var hiddenCount = document.getElementById('hiddenCount');

  var newbtn = document.getElementById('newart');
  var arts = document.getElementById('arts');

  sbtn.addEventListener('click', function()
    {

        var thisText = this.innerHTML;
        

        if (thisText == 'LISTE')
         {
          this.innerHTML="NOUVEAU";
          
          newbloc.style.display = "none";
          listbloc.style.display = "block";
          hidden.value = 'list';
          
        }
        else
        {
           this.innerHTML="LISTE";
         
           listbloc.style.display = "none";
           newbloc.style.display = "block";
           hidden.value = 'new';

        }

     

    });



    newbtn.addEventListener('click', function()
      {

           var artsInner = arts.innerHTML;
           var artsCh = arts.getElementsByTagName('*');

           var count = artsCh.length/2;

          var input = document.createElement('input');
          input.setAttribute('type' , 'text');
          input.setAttribute('name' , 'art'+count);
          input.setAttribute('placeholder' , 'libellé . Ex : Chemise à rayures bleues...');
          input.setAttribute('class' , 'artinput');

          var br = document.createElement('br');
   
          hiddenCount.value = count+1;
 

         arts.appendChild(input);
         arts.appendChild(br);


      });



 




</script>
 

</body></html>

